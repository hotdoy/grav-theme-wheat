{% import "partials/date.html.twig" as date %}
{% extends 'partials/base.html.twig' %}

{% block content %}

    <section id="calendar-heading">
        <div class="wrapper text">
            <h1>{{ page.header.display_title }}</h1>
        </div>
    </section>
    
    <section id="calendar-layout">
        {# {% include "partials/calendar-filter.html.twig" %} #}
        {% include "partials/calendar-list.html.twig" %}
    </section>
    
    {% for module in page.collection({ items: {'@self.modular':''}, 'order': {'by':'folder', 'dir':'asc'}}) %}
    {{ module.content|raw }}
    {% endfor %}

{% endblock %}
