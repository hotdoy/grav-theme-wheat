{# LIBS #}
{% do assets.addJs('theme://libs/swiper-bundle.js', { group:'bottom' }) %}
{% do assets.addJs('theme://libs/scroll-out.js', { group:'bottom' }) %}
{% do assets.addJs('theme://libs/quicklink.js', { group:'bottom'}) %}

{# PAGE-STATE #}
{% do assets.addJs('theme://js/page-state/fx.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/page-state/page-state.js', { group: 'bottom'}) %}
{% do assets.addJs('theme://js/page-state/links.js', { group:'bottom'}) %}

{# PARTIALS #}
{% do assets.addJs('theme://js/partials/header.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/partials/pagination.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/partials/lite-yt-embed.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/partials/dialog.js', { group:'bottom' }) %}

{# MODULES #}
{% do assets.addJs('theme://js/modular/map.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/modular/quotes.js', { group:'bottom' }) %}
{% do assets.addJs('theme://js/modular/carousel.js', { group:'bottom' }) %}

{# PAGES #}
{% do assets.addJs('theme://js/product.js', { group:'bottom' }) %}

{# SNIPCART #}
{% if theme.snipcart %}
{% do assets.addJs('https://cdn.snipcart.com/themes/v' ~ snipcart_version ~ '/default/snipcart.js', { group:'bottom', position: 'after', loading: 'async' }) %}
{% do assets.addJs('theme://js/snipcart.js', { group:'bottom' }) %}
{% endif %}

{# CUSTOM #}
{% do assets.addJs('theme://js/custom.js', { group:'bottom' }) %}
